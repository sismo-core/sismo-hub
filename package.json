{
  "name": "sismo-data-sources",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "npx jest --coverage --runInBand",
    "api": "NODE_ENV=local  LOG=true node --require ts-node/register src/api/local.ts",
    "generate:group": "NODE_ENV=local  LOG=true node --max_old_space_size=4096 --require ts-node/register src/scripts/generate-group.ts",
    "lint": "eslint 'src/**/*.ts' 'attesters/**/*.ts' 'group-generators/**/*.ts' 'hydra-s1/**/*.ts' --max-warnings=0",
    "prettier": "prettier --write 'src/**/*.ts' 'attesters/**/*.ts' 'group-generators/**/*.ts' 'hydra-s1/**/*.ts'"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": ">=6",
    "jest": "^28.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.73.0"
  },
  "lint-staged": {
    "src/**/*": "prettier --write src/**/*.ts",
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint src/**/*.ts --max-warnings=0"
    ]
  },
  "engines": {
    "npm": "NPM IS NOT USED - use yarn instead."
  },
  "dependencies": {
    "@google-cloud/bigquery": "^5.12.0",
    "@sismo-core/crypto": "^1.1.4",
    "@sismo-core/kv-merkle-tree": "^1.0.4",
    "ethers": "^5.6.2",
    "fastify": "^4.3.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0"
  }
}
