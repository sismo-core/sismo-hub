{
  "name": "sismo-data-sources",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "npx jest --coverage --runInBand",
    "lint": "eslint 'src/**/*.ts' 'attesters/**/*.ts' 'group-generators/**/*.ts' --max-warnings=0",
    "prettier": "prettier --write 'src/**/*.ts' 'attesters/**/*.ts' 'group-generators/**/*.ts'",
    "cli": "node --require ts-node/register src/cli/cli.ts",
    "api": "node --require ts-node/register src/cli/cli.ts api",
    "compute-attester": "node --require ts-node/register src/cli/cli.ts compute-attester",
    "generate-group": "node --require ts-node/register src/cli/cli.ts generate-group"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^18.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": ">=6",
    "jest": "^28.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.6.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "src/**/*": "prettier --write src/**/*.ts",
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint src/**/*.ts --max-warnings=0"
    ]
  },
  "engines": {
    "npm": "NPM IS NOT USED - use yarn instead."
  },
  "dependencies": {
    "@fastify/static": "^6.5.0",
    "@fastify/swagger": "^7.4.1",
    "@fastify/type-provider-json-schema-to-ts": "^2.1.0",
    "@google-cloud/bigquery": "^5.12.0",
    "@sismo-core/crypto": "^1.1.4",
    "@sismo-core/kv-merkle-tree": "^1.0.4",
    "commander": "^9.4.0",
    "ethers": "^5.6.2",
    "fastify": "^4.3.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "json-schema-to-ts": "^2.5.5"
  }
}
