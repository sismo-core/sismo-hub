{
  "name": "Github",
  "iconUrl": "",
  "providerClassName": "GithubProvider",
  "functions": [
    {
      "name": "Repositories Contributors",
      "functionName": "getRepositoriesContributors",
      "description": "Return all Github accounts which have contributed to a list of specific repositories",
      "args": [
        {
          "name": "repositories",
          "type": "string[]",
          "example": "sismo-core/sismo-hub, sismo-core/sismo-protocol",
          "description": "List of repositories, comma separated"
        }
      ]
    },
    {
      "name": "Repositories Stargazers",
      "functionName": "getRepositoriesStargazers",
      "description": "Return all Github accounts which have starred a list of specific repositories",
      "args": [
        {
          "name": "repositories",
          "type": "string[]",
          "example": "sismo-core/sismo-hub, sismo-core/sismo-protocol",
          "description": "List of repositories, comma separated"
        }
      ]
    }
  ]
}
