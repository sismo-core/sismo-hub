{
  "name": "Github",
  "iconUrl": "",
  "providerClassName": "GithubProvider",
  "functions": [
    {
      "name": "Get contributors of",
      "functionName": "getRepositoriesContributors",
      "countFunctionName": "getRepositoriesContributorsCount",
      "description": "Returns all Github accounts which have contributed to a list of specific repositories",
      "args": [
        {
          "name": "repositories",
          "argName": "repositories",
          "type": "string[]",
          "example": "sismo-core/sismo-hub, sismo-core/sismo-protocol",
          "description": "List of repositories, comma separated"
        }
      ]
    },
    {
      "name": "Get stargazers of",
      "functionName": "getRepositoriesStargazers",
      "countFunctionName": "getRepositoriesStargazersCount",
      "description": "Returns all Github accounts which have starred a list of specific repositories",
      "args": [
        {
          "name": "repositories",
          "argName": "repositories",
          "type": "string[]",
          "example": "sismo-core/sismo-hub, sismo-core/sismo-protocol",
          "description": "List of repositories, comma separated"
        }
      ]
    }
  ]
}
